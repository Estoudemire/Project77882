public class npc {

    //Attributes
    private String name; 

    /**
     * constructor 
     * @param name the npc's name
     */
    public npc(String name){
        this.name = name; 
    }

    //--METHODS--
    //--SETTERS--
    //--GETTERS--
    /**
     * @return name
     */
    public String getName(){
        return name; 
    }

}